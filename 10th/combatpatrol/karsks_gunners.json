  "enhancements": [
    {
      "description": "In your Command phase, if the bearer is on the battlefield, you gain 1CP. In addition, each time the bearer issues an Order, it is issued to all friendly Astra Militarum units on the battlefield.",
      "detachment": "",
      "excludes": [],
      "id": "417729ff-7351-5e0f-a186-774845b343be",
      "keywords": [
        "Astra Militarum"
      ],
      "name": "Command Laurels"
    },
    {
      "description": "In your Command phase, if the bearer is on the battlefield, you gain 1CP. In addition, while a friendly Artillery unit is within 6\" of the bearer’s unit:\n\n■ Each time you roll to determine the number of attacks a model in that Artillery unit makes with a weapon, you can re-roll the result.\n\n■ Models in that Artillery unit have the Lone Operative ability if they have not made any attacks yet this battle.",
      "detachment": "",
      "excludes": [],
      "id": "4769b0e1-fcab-5071-aca3-b49dd6b333a8",
      "keywords": [
        "Astra Militarum"
      ],
      "name": "Gunnery Officer (Aura)"
    }
  ],
  "id": "5e9a3750-8a32-5df6-94c8-a2acb1b7dd92",
  "isCombatPatrol": true,
  "name": "Combat Patrol: Karsk’s Gunners",
  "stratagems": [
    {
      "cost": 1,
      "detachment": "",
      "effect": "Add a new unit to your army identical to your destroyed unit, at its original Starting Strength. Set that unit up anywhere on the battlefield wholly within 9\" of your battlefield edge and not within Engagement Range of one or more enemy units.",
      "fluff": "Amongst the greatest strengths of the Astra Militarum is its sheer numerical might. There are always more bodies to be hurled into the meat-grinder of battle.",
      "id": "482c346b-3d0d-579e-8de3-38ab7cc120da",
      "name": "Send in the Next Wave",
      "phase": [
        "movement"
      ],
      "restrictions": "",
      "target": "One destroyed Cadian Shock Troops unit from your army. You can use this Stratagem on that unit even though it is destroyed.",
      "turn": "your",
      "type": "Strategic Ploy",
      "when": "Reinforcements step of your Movement phase."
    },
    {
      "cost": 1,
      "detachment": "",
      "effect": "Until the end of the phase, each time a model in one of your selected units makes an attack that targets that enemy unit, you can re-roll the Hit roll.",
      "fluff": "Prioritising a vital enemy target, the Astra Militarum soldiery focus all their fury upon it.",
      "id": "90c5400c-e4e8-5669-93f0-815900b5ffe7",
      "name": "Bring It Down",
      "phase": [
        "shooting"
      ],
      "restrictions": "",
      "target": "Any number of Astra Militarum units from your army, and one enemy unit.",
      "turn": "your",
      "type": "Battle Tactic",
      "when": "Your Shooting phase."
    },
    {
      "cost": 2,
      "detachment": "",
      "effect": "Until the end of the turn, halve the Move characteristic of enemy models, halve Advance rolls made for enemy units, enemy units cannot declare a charge and each time an enemy model makes a ranged attack, subtract 1 from the Hit roll.",
      "fluff": "Big guns behind the Imperial lines rain down a disruption bombardment upon the foe. As the ground shakes and debris fountains skywards, the enemy are thrown into disarray.",
      "id": "71c48149-abe8-5fb4-a7e4-c60eb8b632ca",
      "name": "Artillery Strike",
      "phase": [
        "command"
      ],
      "restrictions": "You can only use this Stratagem once per battle.",
      "target": "One Officer model from your army.",
      "turn": "opponents",
      "type": "Strategic Ploy",
      "when": "Start of your opponent’s Command phase."
    }
  ]
}