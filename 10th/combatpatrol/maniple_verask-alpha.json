  "id": "6f5671e3-3401-5e81-823c-4d6febf592d2",
  "isCombatPatrol": true,
  "name": "Combat Patrol: Maniple Verask-Alpha",
  "stratagems": [
    {
      "cost": 1,
      "detachment": "",
      "effect": "Until the end of the battle, each time an Adeptus Mechanicus model from your army makes an attack that targets that enemy model’s unit, re-roll a Hit roll of 1.",
      "fluff": "Vengeful subroutines within this Tech-Priest’s augmetic architecture ensure that, should they be slain, a purge code is beamed far and wide directing the faithful to hunt down their killer. ",
      "id": "0fd9b4ad-d922-59d0-a15b-8f90d7958a8a",
      "name": "Retribution Codes",
      "phase": [
        "any"
      ],
      "restrictions": "",
      "target": "One Tech-Priest model from your army that was just destroyed by an attack made by an enemy model. You can use this Stratagem on that model even though it was just destroyed.",
      "turn": "either",
      "type": "Strategic Ploy",
      "when": "Any phase."
    },
    {
      "cost": 1,
      "detachment": "",
      "effect": "Until the end of the phase, models in your unit have a 4+ invulnerable save.",
      "fluff": "Triggered by autodetection of incoming enemy fire, single-burst shield capacitors raise a defensive power field around their bearers before burning out in showers of sparks. ",
      "id": "6c88b91f-c033-5271-842c-dd88d64d817e",
      "name": "Reactive Field-shroud",
      "phase": [
        "fight",
        "shooting"
      ],
      "restrictions": "",
      "target": "One Tech-Priest or Skitarii unit from your army that was selected as the target of one or more of the attacking unit’s attacks.",
      "turn": "either",
      "type": "Battle Tactic",
      "when": "Your opponent’s Shooting phase or the Fight phase, just after an enemy unit has selected its targets."
    },
    {
      "cost": 2,
      "detachment": "",
      "effect": "Until the end of the turn, your unit is eligible to shoot in a turn in which it Fell Back.",
      "fluff": "Whether it be by literal ocular implants in the back of their heads, or through advanced cover-target subroutines, this unit is able to lay down deadly accurate volleys to suppress the foe even as they fall back from combat. ",
      "id": "e32124b3-5d48-5dc0-9895-9c114fa548fd",
      "name": "Retrogradial Targeting",
      "phase": [
        "movement"
      ],
      "restrictions": "",
      "target": "That Tech-Priest unit.",
      "turn": "your",
      "type": "Strategic Ploy",
      "when": "Your Movement phase, just after a Tech-Priest unit from your army ends a Fall Back move."
    }
  ]
