  "stratagems": [
    {
      "cost": 1,
      "detachment": "",
      "effect": "Until the end of the phase, each time a model in your unit makes an attack that targets the closest eligible target, you can re-roll the Hit roll.",
      "fluff": "The Black Templars’ disgust for the heresies of their foes lends punishing might to their blows.",
      "id": "5a5e11b8-bb82-50dd-8f2e-2137565d237e",
      "name": "Hate-Fuelled Strength",
      "phase": [
        "fight",
        "shooting"
      ],
      "restrictions": "",
      "target": "One Adeptus Astartes Infantry unit from your army that has not been selected to shoot or fight this phase.",
      "turn": "either",
      "type": "Battle Tactic",
      "when": "Your Shooting phase or the Fight phase."
    },
    {
      "cost": 1,
      "detachment": "",
      "effect": "Until the end of the phase, each time an attack targets your unit, if the attacking model is within 6\" of your unit, models in your unit have a 4+ invulnerable save against that attack. Primaris Neophyte models have a 5+ invulnerable save against that attack instead.",
      "fluff": "When prosecuting the foe at close quarters, the zealous aggression of the Black Templars reaches such fever pitch that they can simply ignore pain and even severe injury.",
      "id": "ea467f2b-6cda-5cdb-8c8e-d0ebe72d15b7",
      "name": "Shield of Zeal",
      "phase": [
        "fight",
        "shooting"
      ],
      "restrictions": "",
      "target": "One Adeptus Astartes Infantry unit from your army that was selected as the target of one or more of the attacking unit’s attacks.",
      "turn": "either",
      "type": "Battle Tactic",
      "when": "Your opponent’s Shooting phase or the Fight phase, just after an enemy unit has selected its targets."
    },
    {
      "cost": 1,
      "detachment": "",
      "effect": "Your unit can make a Normal move, but must end that move as close as possible to that enemy unit.",
      "fluff": "To fire upon the Black Templars is to stoke their outrage that the enemies of Mankind would dare raise arms against the God-Emperor’s chosen warriors. Their response is typically as aggressive as it is immediate.",
      "id": "c3516947-f445-5374-93c0-03ccfb48f980",
      "name": "Goaded to Fury",
      "phase": [
        "shooting"
      ],
      "restrictions": "",
      "target": "One Adeptus Astartes Infantry unit from your army that was selected as the target of one or more of the attacking unit’s attacks.",
      "turn": "opponents",
      "type": "Strategic Ploy",
      "when": "Your opponent’s Shooting phase, just after an enemy unit has shot."
    }
