  "id": "0200b2f7-573e-5ca3-a4dc-cc713ab5e201",
  "isCombatPatrol": true,
  "name": "Combat Patrol: Dark Zealots",
  "stratagems": [
    {
      "cost": 1,
      "detachment": "",
      "effect": "Until the end of the phase, each time a model in your unit is destroyed, if that model has not fought this phase, roll one D6: on a 4+, do not remove it from play. The destroyed model can fight after the attacking model’s unit has finished making its attacks, and is then removed from play.",
      "fluff": "Fuelled by twisted faith and undying hatred, these warriors strike out with spiteful fury even as they are cut down.",
      "id": "bc543c06-a378-5a71-b0e1-72f1f46dbe1a",
      "name": "Bitter Blows",
      "phase": [
        "fight"
      ],
      "restrictions": "",
      "target": "One Heretic Astartes Infantry unit from your army that was selected as the target of one or more of the attacking unit’s attacks.",
      "turn": "either",
      "type": "Battle Tactic",
      "when": "Fight phase, just after an enemy unit has selected its targets."
    },
    {
      "cost": 1,
      "detachment": "",
      "effect": "Until the end of the phase, each time a model in your unit makes an attack, re-roll a Hit roll of 1. If that attack targets a unit that is Below Half-strength, you can re-roll the Hit roll instead.",
      "fluff": "Veterans of the Long War, these warriors know precisely where to place their shots to inflict the most pain and misery on their luckless victims.",
      "id": "4c1b9e8a-154a-52b1-ac97-1d13bac54666",
      "name": "Malicious Volleys",
      "phase": [
        "shooting"
      ],
      "restrictions": "",
      "target": "One Heretic Astartes Infantry unit from your army that has not been selected to shoot this phase.",
      "turn": "your",
      "type": "Battle Tactic",
      "when": "Your Shooting phase."
    },
    {
      "cost": 1,
      "detachment": "",
      "effect": "Until the end of the phase, models in your unit have a 4+ invulnerable save.",
      "fluff": "Invoking the names of daemonic patrons, the Heretic Astartes ward away enemy attacks.",
      "id": "3985e851-5910-5ec9-89f8-8f2f53089df5",
      "name": "Empyric Rites",
      "phase": [
        "fight",
        "shooting"
      ],
      "restrictions": "",
      "target": "One Heretic Astartes Infantry unit from your army that was selected as the target of one or more of the attacking unit’s attacks.",
      "turn": "either",
      "type": "Battle Tactic",
      "when": "Your opponent’s Shooting phase or the Fight phase, just after an enemy unit has selected its targets."
    }
  ]
