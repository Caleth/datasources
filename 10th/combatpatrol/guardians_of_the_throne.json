  "stratagems": [
    {
      "cost": 1,
      "detachment": "",
      "effect": "If your unit is not within Engagement Range of one or more enemy models, it can make a Normal move.",
      "fluff": "The Adeptus Custodes drive their foes before them, relentlessly dogging their heels as they run their quarry to ground and destroy them utterly.",
      "id": "92f6413e-3581-5b2c-820e-7d57e53aa303",
      "name": "Tireless Pursuit",
      "phase": [
        "movement"
      ],
      "restrictions": "",
      "target": "One Adeptus Custodes Infantry unit from your army that was within Engagement Range of that enemy unit at the start of the phase.",
      "turn": "opponents",
      "type": "Strategic Ploy",
      "when": "Your opponent’s Movement phase, just after an enemy unit ends a Fall Back move."
    },
    {
      "cost": 1,
      "detachment": "",
      "effect": "Until the end of the battle, ranged weapons equipped by Adeptus Custodes models from your army have the [SUSTAINED HITS 1] ability while targeting that enemy model’s unit.",
      "fluff": "The Adeptus Custodes are storied champions who know one another’s worth only too well. They are quick to avenge the deaths of the greatest amongst them.",
      "id": "e6779bba-e04b-5bfc-9215-b4a402f25f55",
      "name": "Wrath of the Host",
      "phase": [
        "any"
      ],
      "restrictions": "",
      "target": "One Shield-Captain model from your army that was just destroyed by an attack made by an enemy model. You can use this Stratagem on that model even though it was just destroyed.",
      "turn": "either",
      "type": "Battle Tactic",
      "when": "Any phase"
    },
    {
      "cost": 2,
      "detachment": "",
      "effect": "Until the start of your next Command phase, add 1 to the Objective Control characteristic of models in your unit.",
      "fluff": "The sheer auramite-clad glory and physical menace of the Adeptus Custodes is more than enough to overwhelm a more numerous foe.",
      "id": "38111dd7-860b-5157-846b-5a53622f5425",
      "name": "Gilded Magnificence",
      "phase": [
        "command"
      ],
      "restrictions": "",
      "target": "One Adeptus Custodes unit from your army.",
      "turn": "your",
      "type": "Strategic Ploy",
      "when": "Your Command phase."
    }
